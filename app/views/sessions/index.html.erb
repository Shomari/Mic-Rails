<div class="container-fluid session_container">
	<h1 class="text-center" id="notification"></h1>
	<div class="row">
		<div class="col-xs-8 col-xs-offset-2">
			<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
			<!-- Recents -->
			<ins class="adsbygoogle"
		     style="display:inline-block;width:728px;height:90px"
		     data-ad-client="ca-pub-9121927266889461"
		     data-ad-slot="1728247488"></ins>
			<script>
			(adsbygoogle = window.adsbygoogle || []).push({});
			</script>
		</div>
	</div>
	<div class="row">
	<% @sessions.each_with_index do |session, index| %>
		<% if index < 3 %>
				<div class="col-md-4 session_box">
					<%= render partial: "online", locals: {session: session} %>
				</div>
		<% else %>
			</div>
			<div class="row">
				<div class="col-md-4 session_box">
					<%= render partial: "online", locals: {session: session} %>
				</div>
		<% end %>
	<% end %>

	</div>
</div>